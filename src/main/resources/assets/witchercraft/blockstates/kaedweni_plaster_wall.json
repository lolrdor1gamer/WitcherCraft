{
  "multipart": [
    { "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_post" } },
    {
      "when": { "north": "true" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_side" }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_side", "y": 180 }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_side_alt", "y": 270 }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_side_alt", "y": 90 }
    },
    {
      "when": { "north": "false" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_noside" }
    },
    {
      "when": { "south": "false" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_noside", "y": 180 }
    },
    {
      "when": { "west": "false" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_noside_alt", "y": 270 }
    },
    {
      "when": { "east": "false" },
      "apply": { "model": "witchercraft:block/kaedweni_plaster_wall_noside_alt", "y": 90 }
    }
  ]
}
